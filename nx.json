{
  "npmScope": "scop",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "api": {
      "tags": []
    },
    "entities": {
      "tags": []
    },
    "environment": {
      "tags": []
    },
    "globals": {
      "tags": []
    },
    "interfaces": {
      "tags": []
    },
    "migrations": {
      "tags": []
    },
    "mobile": {
      "tags": []
    },
    "services": {
      "tags": []
    },
    "subscribers": {
      "tags": []
    },
    "web": {
      "tags": []
    }
  }
}
